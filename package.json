{
	"name": "not-dbdump",
	"version": "0.0.11",
	"description": "not-* family module to dump/restore mongodb of project",
	"main": "src/index.js",
	"bin": {
		"not-dump": "./src/bin/dump.sh",
		"not-restore": "./src/bin/restore.sh"
	},
	"scripts": {
		"lint": "/usr/local/bin/eslint --ext svelte3 ./src/**/**/**/**",
		"pretest": "npm run lint",
		"docs": "./node_modules/.bin/jsdoc -c jsdoc.json"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/interrupter/not-dbdump.git"
	},
	"keywords": [
		"not-bulma",
		"not-node",
		"components",
		"bulma"
	],
	"author": "interrupter <alex.repin@mail.ru> (http://github.com/interrupter)",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/interrupter/not-dbdump/issues"
	},
	"homepage": "https://github.com/interrupter/not-dbdump#readme",
	"dependencies": {
		"filesize": "*",
		"not-bulma": "*",
		"not-log": "*"
	},
	"devDependencies": {
		"eslint": "*"
	},
	"nyc": {
		"include": [
			"index.js",
			"src",
			"src/**/**/**/**.svelte",
			"test/browser/assets/bulma/**.js"
		],
		"exclude": [
			"test/**/**/**/**.js"
		],
		"reporter": [
			"html"
		],
		"check-coverage": true,
		"per-file": true,
		"lines": 95,
		"statements": 95,
		"functions": 95,
		"branches": 95,
		"watermarks": {
			"lines": [
				80,
				95
			],
			"functions": [
				80,
				95
			],
			"branches": [
				80,
				95
			],
			"statements": [
				80,
				95
			]
		}
	}
}